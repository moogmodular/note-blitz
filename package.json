{
    "name": "note-blitz",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build:1-generate": "prisma generate",
        "build:2-migrate": "prisma migrate deploy",
        "build:3-next": "cross-env NODE_ENV=production next build",
        "build:4-server": "tsc --project tsconfig.server.json",
        "build": "run-s build:*",
        "dev:wss": "cross-env PORT=3001 nodemon --watch src --ext .ts,.tsx,js,jsx --signal SIGTERM --exec \"ts-node --project tsconfig.server.json src/server/wssDevServer.ts\"",
        "dev:next": "next dev",
        "dev": "run-p dev:*",
        "db-up": "docker-compose up -d",
        "db-seed": "prisma db seed",
        "db-migrate-dev": "yarn prisma migrate dev",
        "db-nuke": "docker-compose down --volumes --remove-orphans",
        "dx": "run-s db-up db-migrate-dev db-seed dev",
        "dev-nuke": "rm -rf prisma/*.db**",
        "generate": "prisma generate",
        "migrate-dev": "prisma migrate dev",
        "migrate": "prisma migrate deploy",
        "start": "cross-env NODE_ENV=production node dist/server/prodServer.js",
        "studio": "prisma studio",
        "lint": "eslint src",
        "lint-fix": "yarn lint --fix",
        "test-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
        "test-start": "cross-env APP_ENV=test WS_URL=ws://localhost:3000 start-server-and-test start 3000 test",
        "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
        "test": "jest"
    },
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@formkit/auto-animate": "^1.0.0-beta.1",
        "@mantine/core": "^4.2.10",
        "@mantine/dropzone": "^4.2.10",
        "@mantine/hooks": "^4.2.10",
        "@mantine/next": "^4.2.10",
        "@mantine/rte": "^4.2.10",
        "@mui/material": "^5.8.6",
        "@prisma/client": "^4.0.0",
        "@trpc/client": "^9.25.3",
        "@trpc/next": "^9.25.3",
        "@trpc/react": "^9.25.3",
        "@trpc/server": "^9.25.3",
        "clsx": "^1.1.1",
        "date-fns": "^2.28.0",
        "html-react-parser": "^2.0.0",
        "jsonwebtoken": "^8.5.1",
        "knex": "^2.1.0",
        "lightning": "^5.16.2",
        "lnurl": "^0.24.1",
        "next": "^12.1.6",
        "next-auth": "^4.8.0",
        "node-fetch": "^2.6.1",
        "pg": "^8.7.3",
        "qrcode.react": "^3.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.31.3",
        "react-image-file-resizer": "^0.4.8",
        "react-query": "^3.37.0",
        "start-server-and-test": "^1.12.0",
        "styled-icons": "^10.45.0",
        "superjson": "^1.7.4",
        "ts-node": "^10.3.0",
        "unique-names-generator": "^4.7.1",
        "ws": "^8.0.0",
        "zod": "^3.0.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.1.0",
        "@types/jest": "^27.0.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.10",
        "@types/node-fetch": "^2.5.11",
        "@types/react": "^18.0.9",
        "@types/styled-components": "^5.1.25",
        "@types/ws": "^8.2.0",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^5.14.0",
        "autoprefixer": "^10.4.4",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-next": "^12.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "import-sort-style-module": "^6.0.0",
        "jest": "^27.1.0",
        "jest-playwright": "^0.0.1",
        "jest-playwright-preset": "^1.4.5",
        "nodemon": "^2.0.12",
        "npm-run-all": "^4.1.5",
        "playwright": "^1.19.1",
        "postcss": "^8.4.12",
        "prettier": "^2.6.2",
        "prettier-plugin-import-sort": "^0.0.7",
        "prisma": "^4.0.0",
        "styled-components": "^5.3.5",
        "ts-jest": "^27.0.5",
        "typescript": "4.4.4"
    },
    "publishConfig": {
        "access": "restricted"
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module",
            "parser": "typescript"
        }
    },
    "prisma": {
        "seed": "yarn ts-node prisma/seed.ts"
    }
}
