DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432
DB_NAME=lnapp

# Make sure to override these in deployment
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/lnapp?schema=public"
APP_URL="http://localhost:3000"
WS_URL="ws://localhost:3001"
NEXTAUTH_URL="http://localhost:3000/api/auth"
NEXTAUTH_SECRET="123456"

# local ip address of the server running the app (reacable for a cellphone for testing ln-urls in development)
SERVER_HOST=192.168.0.199
#SERVER_HOST=192.168.43.173
SERVER_PORT=3000

NEXT_PUBLIC_SERVER_HOST=localhost
NEXT_PUBLIC_SERVER_PORT=3000

LNURL_SERVER_HOST=localhost
LNURL_SERVER_PORT=3002

